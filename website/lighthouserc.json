{
  "ci": {
    "collect": {
      "staticDistDir": "./website",
      "numberOfRuns": 2,
      "url": [
        "http://localhost/",
        "http://localhost/memberships/",
        "http://localhost/kiln-firing/",
        "http://localhost/studio-access/",
        "http://localhost/support/"
      ]
    },
    "assert": {
      "assertions": {
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "categories:best-practices": [
          "warn",
          {
            "minScore": 0.85
          }
        ]
      }
    }
  }
}
